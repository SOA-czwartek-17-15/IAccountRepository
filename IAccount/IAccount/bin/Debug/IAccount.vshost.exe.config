<?xml version="1.0"?>
<configuration>
<startup useLegacyV2RuntimeActivationPolicy="true">
  <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5"/>
</startup>

<appSettings>
  <add key="ServiceRepositoryAddress" value="net.tcp://localhost:54321/ServiceRepo"/>
  <add key="AccountServiceAddress" value="net.tcp://localhost:41266/IAccount"/>
  <add key="KeepAliveInterval" value="10000"/>

  <add key="log4net.Config" value="log4.config"/>
  <add key="log4net.Config.Watch" value="True"/>
  <add key="log4net.Internal.Debug" value="False"/>
</appSettings>
</configuration>